<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_dashboard.xml"
    app:startDestination="@id/chatFragment">

    <fragment
        android:id="@+id/profileFragment"
        android:name="com.fictivestudios.demo.ui.fragments.main.profile.ProfileFragment"
        android:label="ProfileFragment"
        tools:layout="@layout/fragment_profile">
        <action
            android:id="@+id/action_profileFragment_to_editProfileFragment"
            app:destination="@id/editProfileFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_profileFragment_to_imagePreviewFragment"
            app:destination="@id/imagePreviewFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right">
            <argument
                android:name="imageLink"
                app:argType="string" />

            <argument
                android:name="from"
                app:argType="string" />
        </action>
    </fragment>

    <fragment
        android:id="@+id/chatFragment"
        android:name="com.fictivestudios.demo.ui.fragments.main.chat.ChatFragment"
        android:label="ChatFragment"
        tools:layout="@layout/fragment_chat">

        <action
            android:id="@+id/action_chatFragment_to_userProfileFragment"
            app:destination="@id/userProfileFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right">

            <argument
                android:name="userId"
                app:argType="string" />

        </action>
        <action
            android:id="@+id/action_chatFragment_to_chatDetailFragment"
            app:destination="@id/chatDetailFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/addressFragment"
        android:name="com.fictivestudios.demo.ui.fragments.main.address.AddressFragment"
        android:label="AddressFragment"
        tools:layout="@layout/fragment_address">
        <action
            android:id="@+id/action_addressFragment_to_createNewContactFragment"
            app:destination="@id/createNewContactFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_addressFragment_to_userProfileFragment"
            app:destination="@id/userProfileFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right">

            <argument
                android:name="userId"
                app:argType="string" />

        </action>
    </fragment>
    <fragment
        android:id="@+id/dialPadFragment"
        android:name="com.fictivestudios.demo.ui.fragments.main.dialPad.DialPadFragment"
        android:label="DialPadFragment"
        tools:layout="@layout/fragment_dial_pad" />
    <fragment
        android:id="@+id/editProfileFragment"
        android:name="com.fictivestudios.demo.ui.fragments.main.profile.EditProfileFragment"
        android:label="EditProfileFragment"
        tools:layout="@layout/fragment_edit_profile" />
    <fragment
        android:id="@+id/notificationFragment"
        android:name="com.fictivestudios.demo.ui.fragments.main.notification.NotificationFragment"
        android:label="NotificationFragment"
        tools:layout="@layout/fragment_notification" />
    <fragment
        android:id="@+id/createNewContactFragment"
        android:name="com.fictivestudios.demo.ui.fragments.main.address.CreateNewContactFragment"
        android:label="CreateNewContactFragment"
        tools:layout="@layout/fragment_new_contact" />
    <fragment
        android:id="@+id/userProfileFragment"
        android:name="com.fictivestudios.demo.ui.fragments.main.profile.UserProfileFragment"
        android:label="UserProfileFragment"
        tools:layout="@layout/fragment_user_profile">

        <argument
            android:name="userId"
            app:argType="string" />
        <action
            android:id="@+id/action_userProfileFragment_to_imagePreviewFragment"
            app:destination="@id/imagePreviewFragment" >

            <argument
                android:name="imageLink"
                app:argType="string" />

            <argument
                android:name="from"
                app:argType="string" />

        </action>


    </fragment>
    <fragment
        android:id="@+id/chatDetailFragment"
        android:name="com.fictivestudios.demo.ui.fragments.main.chat.ChatDetailFragment"
        android:label="ChatDetailFragment"
        tools:layout="@layout/fragment_chat_detail">
        <action
            android:id="@+id/action_chatDetailFragment_to_userProfileFragment"
            app:destination="@id/userProfileFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right">

            <argument
                android:name="userId"
                app:argType="string" />

        </action>
    </fragment>
    <fragment
        android:id="@+id/settingFragment"
        android:name="com.fictivestudios.demo.ui.fragments.main.settings.SettingFragment"
        android:label="SettingFragment"
        tools:layout="@layout/fragment_setting">
        <action
            android:id="@+id/action_settingFragment_to_blockUserFragment"
            app:destination="@id/blockUserFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_settingFragment_to_appLockManagerFragment"
            app:destination="@id/appLockManagerFragment" />
    </fragment>

    <fragment
        android:id="@+id/blockUserFragment"
        android:name="com.fictivestudios.demo.ui.fragments.main.blockUser.BlockUserFragment"
        android:label="BlockUserFragment"
        tools:layout="@layout/fragment_block_user" />
    <fragment
        android:id="@+id/privacyPolicyFragment2"
        android:name="com.fictivestudios.demo.ui.fragments.auth.privacy.PrivacyPolicyFragment"
        android:label="PrivacyPolicyFragment"
        tools:layout="@layout/fragment_privacy" />
    <fragment
        android:id="@+id/imagePreviewFragment"
        android:name="com.fictivestudios.demo.ui.fragments.main.previewFragment.ImagePreviewFragment"
        android:label="ImagePreviewFragment"
        tools:layout="@layout/fragment_image_preview">

        <argument
            android:name="imageLink"
            app:argType="string" />
        <argument
            android:name="from"
            app:argType="string" />

        <argument
            android:name="attachmentId"
            app:argType="string"
            app:nullable="true" />
    </fragment>
    <fragment
        android:id="@+id/callLogFragment"
        android:name="com.fictivestudios.demo.ui.fragments.main.callLog.CallLogFragment"
        android:label="CallLogFragment"
        tools:layout="@layout/fragment_call_log" />


    <fragment
        android:id="@+id/patternFragment"
        android:name="com.fictivestudios.demo.ui.fragments.main.patternFragment.PatternFragment"
        android:label="PatternFragment"
        tools:layout="@layout/fragment_pattern" />
    <fragment
        android:id="@+id/pinFragment"
        android:name="com.fictivestudios.demo.ui.fragments.main.pinFragment.PinFragment"
        android:label="PinFragment"
        tools:layout="@layout/fragment_pin" />
    <fragment
        android:id="@+id/appLockManagerFragment"
        android:name="com.fictivestudios.demo.ui.fragments.main.appLockManager.AppLockManagerFragment"
        android:label="AppLockManager"
        tools:layout="@layout/fragment_app_lock_manager">
        <action
            android:id="@+id/action_appLockManager_to_patternFragment"
            app:destination="@id/patternFragment" />
        <action
            android:id="@+id/action_appLockManager_to_pinFragment"
            app:destination="@id/pinFragment" />
    </fragment>
    <fragment
        android:id="@+id/registerPhoneNo"
        android:name="com.fictivestudios.demo.ui.fragments.main.registerPhoneNo.FragmentRegisterPhoneNo"
        android:label="RegisterPhoneNo" />
    <fragment
        android:id="@+id/screenShotFragment"
        android:name="com.fictivestudios.demo.ui.fragments.main.screenShot.ScreenShotFragment"
        android:label="ScreenShotFragment">
        <action
            android:id="@+id/action_screenShotFragment_to_imagePreviewFragment"
            app:destination="@id/imagePreviewFragment">
            <argument
                android:name="imageLink"
                app:argType="string" />
            <argument
                android:name="from"
                app:argType="string" />

            <argument
                android:name="attachmentId"
                app:argType="string" />
        </action>
    </fragment>
    <fragment
        android:id="@+id/securityFeatureFragment"
        android:name="com.fictivestudios.demo.ui.fragments.main.securityFeature.SecurityFeatureFragment"
        android:label="SecurityFeatureFragment">
        <action
            android:id="@+id/action_securityFeatureFragment_to_imagePreviewFragment"
            app:destination="@id/imagePreviewFragment">
            <argument
                android:name="imageLink"
                app:argType="string" />

            <argument
                android:name="from"
                app:argType="string" />

            <argument
                android:name="attachmentId"
                app:argType="string" />
        </action>

        <action
            android:id="@+id/action_securityFeatureFragment_to_exoPlayerFragment"
            app:destination="@id/exoPlayerFragment">
            <argument
                android:name="url"
                app:argType="string" />
        </action>
    </fragment>

    <fragment
        android:id="@+id/exoPlayerFragment"
        android:name="com.fictivestudios.demo.ui.fragments.main.player.ExoPlayerFragment"
        android:label="ExoPlayerFragment">
        <argument
            android:name="url"
            app:argType="string" />
    </fragment>
</navigation>